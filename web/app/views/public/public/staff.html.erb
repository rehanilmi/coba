<main role="main">

  <!-- Main jumbotron for a primary marketing message or call to action -->
  <div class="jumbotron">
    <div class="container">
      <section id="staff" class="nb-5">
      <div class="container" style=text-align:center>
        <div class="row">
          <% @stafs.each do |staf| %>
          <div class="col">
              <div>
                <img src="<%= staf.staf_photo %>" style="width: 200px">
              </div>

              <div><%= staf.staf_nama %></div>
              <div><%= staf.staf_email %></div>
              <div><%= staf.staf_telepon %></div>
              <div><%= staf.staf_alamat %></div>
          </div>
          <% end %>
        </div>
      </div>
      </section>
