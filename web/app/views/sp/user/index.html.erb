<div class="table-responsive">
<table class="table table-striped table-bordered">
  <thead>
    <tr>

      <th>#</th>
      <th>Nama</th>
      <th>Email</th>
      <th>Telepon</th>
    </tr>
  </thead>
  <tbody>

    <% @datas.each do |f| %>
    <tr>
      <td><%= f.id %></td>
      <td><%= f.oadm_nama %></td>
      <td><%= f.oadm_email %></td>
      <td><%= f.oadm_telepon %></td>


    </tr>
    <% end %>


  </tbody>
</table>
</div>
